  "keywords": [
    "react",
    "react-component",
    "background-video",
    "html5-video",
    "video-background",
    "react-background-video",
    "hero-video",
    "landing-page-video",
    "fullscreen-video",
    "responsive-video",
    "webm",
    "mp4",
    "ui-component",
    "frontend",
    "react-ui"
  ],
  "version": "1.4.3",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepack": "npm run build",
    "clean": "rm -rf dist",
    "packup": "npm run clean && npm run build && npm pack",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --verbose"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "license": "MIT",
  "dependencies": {
    "react-device-detect": "^2.2.3",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.56.0",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.97.3",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "repository": {
    "type": "git",
